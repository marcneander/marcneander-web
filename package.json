{
  "name": "marcneander.se",
  "version": "1.0.0",
  "description": "Freelancing front-end engineer",
  "author": "Marc Neander <marcneander@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/marcneander/marcneander.se"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "yarn run lint",
      "git add"
    ]
  },
  "keywords": [
    "marcneander",
    "react",
    "gatsby"
  ],
  "scripts": {
    "precommit": "lint-staged",
    "lint": "yarn run lint:css && yarn run lint:js",
    "lint:js": "eslint src",
    "lint:css": "stylelint './src/**/*.js'",
    "build": "gatsby build",
    "serve": "gatsby serve -H 0.0.0.0",
    "develop": "gatsby develop -H 0.0.0.0",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.8.0",
    "classnames": "^2.2.6",
    "gatsby": "2.0.18",
    "gatsby-plugin-layout": "1.0.3",
    "gatsby-plugin-offline": "2.0.5",
    "gatsby-plugin-react-helmet": "3.0.0",
    "gatsby-plugin-styled-components": "^3.0.0",
    "polished": "^2.2.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.5.2",
    "react-helmet": "^5.2.0",
    "react-transition-group": "^2.5.0",
    "styled-components": "^4.0.0-beta.9"
  },
  "devDependencies": {
    "@marcneander/eslint-config-react": "^1.0.0",
    "eslint": "5.6.1",
    "husky": "^1.1.0",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.3",
    "stylelint": "^9.6.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.4.0"
  }
}
